<div class="cell-block-y">
  <div class="grid-container">
  <p id="notice"><%= notice %></p>

  <div class="inner-title cell-block-x small-9">
    <span class="uline"><h1>CHECK IN</h1> </span>
  </div>

  <%# unless @today_return.nil? || @today_return.empty? %>
    <div class="rounded information-table">
      <table class="information-table">
        <thead>
          <tr>
            <th>School</th>
            <th style="text-align:center;">Teacher Renting</th>
            <th style="text-align:center;">Kits being Rented</th>
            <th></th>
          </tr>
        </thead>

        <tbody>
          <% @today_return.each do |returnKit| %>
            <tr>
              <td><%= returnKit.school.name %></td>
              <td style="text-align:center;"><%= returnKit.teacher.full_name %></td>
              <td style="text-align:center;"><%= returnKit.item_category.name %></td>
              <td style="text-align:right;"><i class="fas fa-caret-down"></i></td>
            </tr>
            <div id="">
              <tr>
            <div>
          <% end %>
        </tbody>
      </table>
    </div>
    <%# else %>
      <!-- <p><strong>All kits returned. Good work!</strong></p> -->
    <%# end %>
  </div>
</div>