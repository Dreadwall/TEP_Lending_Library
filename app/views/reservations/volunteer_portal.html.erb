    </div>

    <div class="cell-block-y">
      <div class="grid-x grid-margin-x">
      <div class="cell-block-container cell auto">
        <div class="cell-block-y small-6 button radius" id="component-block">
          <h5><%= link_to "CHECK OUT", pickup_path %></h5>
        </div>
      </div>
      <div class="cell-block-container cell auto">
        <div class="cell-block-y small-6 button radius" id="component-block">
          <h5><%= link_to "CHECK IN", returns_path %></h5>
        </div>
      </div>
    </div>
  </div>
 </div>


